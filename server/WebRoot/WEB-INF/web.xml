<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<display-name>PutHereTheNameOfYourWebApp</display-name>
	<description>This a description of my web app made by Eclipse</description>
	<servlet>
		<servlet-name>EchoStream</servlet-name>
		<servlet-class>EchoStream</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ChatStream</servlet-name>
		<servlet-class>ChatStream</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BroadcastStream</servlet-name>
		<servlet-class>BroadcastStream</servlet-class>
	</servlet>
  <servlet>
    <servlet-name>DrawNGuessServlet</servlet-name>
    <servlet-class>drawNGuess.DrawNGuessServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EnterServlet</servlet-name>
    <servlet-class>drawNGuess.EnterServlet</servlet-class>
  </servlet>
	 <servlet>
    <servlet-name>FliveInARowServlet</servlet-name>
    <servlet-class>fiveInARow.FliveInARowServlet</servlet-class>
  </servlet>



	<servlet-mapping>
		<servlet-name>EchoStream</servlet-name>
		<url-pattern>/EchoStream</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ChatStream</servlet-name>
		<url-pattern>/ChatStream</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BroadcastStream</servlet-name>
		<url-pattern>/BroadcastStream</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>DrawNGuessServlet</servlet-name>
    <url-pattern>/DrawNGuessServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EnterServlet</servlet-name>
    <url-pattern>/EnterServlet</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>FliveInARowServlet</servlet-name>
    <url-pattern>/five</url-pattern>
  </servlet-mapping>
	<welcome-file-list>
		<welcome-file>/index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
